#include <stdio.h>

int main() {
    int matriz[3][3] = {
        {1000, 999, 1222},
        {1223, 22, 311},
        {3002, 3001, 344}
    };

    int maior = matriz[0][0];  

    for (int i = 0; i < 3; i++) {
        for (int j = 0; j < 3; j++) {
            if (matriz[i][j] > maior) {
                maior = matriz[i][j];  
            }
        }
    }

    printf("O maior número da matriz é: %d\n", maior);

    return 0;
}
