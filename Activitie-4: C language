#include <stdio.h>

int main() {
    int num[6];
    int soma = 0;
    float media;

    for (int i = 0; i < 6; i++) {
        printf("Digite o número %d: ", i + 1);
        scanf("%d", &num[i]);
        soma += num[i];
    }
    
    media = (float)soma / 6;
    
    printf("A média dos seis números é: %.2f\n", media);
    
    return 0;
}
