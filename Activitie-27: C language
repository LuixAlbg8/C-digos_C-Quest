#include <stdio.h>

int main() {
    float preco, preco_com_inflacao;

    printf("Digite o preço do produto: ");
    scanf("%f", &preco);

    if (preco < 100) {
    preco_com_inflacao = preco * 1.10;  
    } else {
   preco_com_inflacao = preco * 1.20; 
    }

    printf("Preço após inflação: %.2f\n", preco_com_inflacao);

    return 0;
}
